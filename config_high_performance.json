{
  "backbone": "swin",
  "num_classes": 3,
  "tuning_method": "full",
  "loss_function": "wbce",
  "attention": "none",
  "label_smoothing_epsilon": 0.1,
  "mixup_alpha": 0.2,
  "train_csv": "train.csv",
  "val_csv": "val.csv",
  "test_csv": "offsite_test.csv",
  "train_image_dir": "./images/train",
  "val_image_dir": "./images/val",
  "test_image_dir": "./images/offsite_test",
  "num_workers": 4,
  "early_stopping_patience": 20,
  "epochs": 50,
  "batch_size": 32,
  "lr": 1e-4,
  "weight_decay": 1e-4,
  "use_cosine_scheduler": true,
  "img_size": 256,
  "use_randaugment": true,
  "randaugment_num_ops": 2,
  "randaugment_magnitude": 10,
  "use_ema": true,
  "ema_decay": 0.9997,
  "use_ema_for_eval": true,
  "use_tta_for_eval": true,
  "save_dir": "checkpoints",
  "pretrained_backbone": "./pretrained_backbone/ckpt_swin_base.pt",
  "do_train": true,
  "do_test": true,
  "do_predict": true,
  "predict_input_csv": "./onsite_test.csv",
  "predict_image_dir": "./images/onsite_test",
  "predict_output_csv": "./onsite_test_submission.csv"
}
